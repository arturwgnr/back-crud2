datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Profile {
  id         Int      @id @default(autoincrement())
  name       String
  username   String
  bio        String?
  profession String?
  online     Boolean  @default(false)
  createdAt  DateTime @default(now())
}
